{
    "name": "mikelgoig/codeception-openapi",
    "description": "Codeception module for contract testing with OpenAPI.",
    "license": "MIT",
    "type": "library",
    "version": "2.0.1",
    "keywords": [
        "codeception",
        "contract-testing",
        "dev",
        "gherkin",
        "openapi",
        "rest",
        "symfony",
        "testing"
    ],
    "authors": [
        {
            "name": "Mikel Goig",
            "email": "m@mikelgoig.com"
        }
    ],
    "require": {
        "php": "8.3.*",
        "codeception/codeception": "^5.1",
        "codeception/module-rest": "^3.4",
        "codeception/module-symfony": "^3.5",
        "guzzlehttp/psr7": "^2.7",
        "league/openapi-psr7-validator": "^0.22.0",
        "symfony/browser-kit": "^7.2"
    },
    "require-dev": {
        "captainhook/captainhook-phar": "^5.24",
        "captainhook/hook-installer": "^1.0",
        "mikelgoig/easy-coding-standard-rules": "^1.0",
        "ramsey/conventional-commits": "^1.5",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "MikelGoig\\Codeception\\Module\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "captainhook/captainhook-phar": true,
            "captainhook/hook-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "ecs": "vendor/bin/ecs check --ansi",
        "ecs-fix": "vendor/bin/ecs check --fix --ansi"
    }
}
